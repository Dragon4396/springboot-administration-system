ollapse() {\r\n      this.isCollapse = !this.isCollapse\r\n      if (this.isCollapse) {\r\n        this.sideWidth = 64\r\n        this.collapseBtnClass = 'el-icon-s-unfold'\r\n        this.logoTextShow = false\r\n      } else {\r\n        this.sideWidth = 200\r\n        this.collapseBtnClass = 'el-icon-s-fold'\r\n        this.logoTextShow = true\r\n      }\r\n    },\r\n    load() {\r\n      this.request.get(\"http://localhost:9090/user/page?pageNum=\"+this.pageNum+\r\n          \"&pageSize=\"+this.pageSize +\"&username=\"+this.username).then(res=> {\r\n        console.log(res)\r\n        this.tableData = res.records\r\n        this.total = res.total\r\n      })\r\n    },\r\n    handleSizeChange(pageSize) {\r\n      console.log(pageSize)\r\n      this.pageSize = pageSize\r\n      this.load()\r\n    },\r\n    handleCurrentChange(pageNum) {\r\n      console.log(pageNum)\r\n      this.pageNum = pageNum\r\n      this.load()\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.headerBg {\r\n  background: #eee!important;\r\n}\r\n</style>\r\n"],"mappings":"AAyHA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,KAAA;MACAC,OAAA;MACAC,QAAA;MACAC,QAAA;MACAC,GAAA;MACAC,gBAAA;MACAC,UAAA;MACAC,SAAA;MACAC,YAAA;MACAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,OAAA;IACAC,SAAA;MACA,KAAAP,UAAA,SAAAA,UAAA;MACA,SAAAA,UAAA;QACA,KAAAC,SAAA;QACA,KAAAF,gBAAA;QACA,KAAAG,YAAA;MACA;QACA,KAAAD,SAAA;QACA,KAAAF,gBAAA;QACA,KAAAG,YAAA;MACA;IACA;IACAG,KAAA;MACA,KAAAG,OAAA,CAAAC,GAAA,mDAAAd,OAAA,GACA,oBAAAC,QAAA,uBAAAC,QAAA,EAAAa,IAAA,CAAAC,GAAA;QACAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;QACA,KAAAlB,SAAA,GAAAkB,GAAA,CAAAG,OAAA;QACA,KAAApB,KAAA,GAAAiB,GAAA,CAAAjB,KAAA;MACA;IACA;IACAqB,iBAAAnB,QAAA;MACAgB,OAAA,CAAAC,GAAA,CAAAjB,QAAA;MACA,KAAAA,QAAA,GAAAA,QAAA;MACA,KAAAS,IAAA;IACA;IACAW,oBAAArB,OAAA;MACAiB,OAAA,CAAAC,GAAA,CAAAlB,OAAA;MACA,KAAAA,OAAA,GAAAA,OAAA;MACA,KAAAU,IAAA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}